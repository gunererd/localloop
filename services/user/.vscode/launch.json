{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch User Management Service",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "env": {
                "PORT": "8080",
                "JWT_SECRET": "your-default-secret-key",
                "JWT_EXPIRATION_MINUTES": "1440",
                "SALT_LENGTH": "25"
            }
        }
    ]
} 